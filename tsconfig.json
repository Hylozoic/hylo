{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "checkJs": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"]
    },
    "outDir": "built"
  },
  "exclude": [
    "node_modules",
    "es5",
    "build"
  ],
  "include": [
    "./**/*",
    "./*",
    ".eslintrc.js"
  ]
}
