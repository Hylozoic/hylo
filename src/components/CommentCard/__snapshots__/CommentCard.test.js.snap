// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`displays image an image attachments 1`] = `
<a
  data-stylename="link"
  onClick={[Function]}
>
  <div
    data-stylename="comment-card"
  >
    <div
      data-stylename="comment-header"
    >
      <RoundImage
        large={true}
        url="foo.jpg"
      />
      <Highlight
        component="span"
        highlightClassName="highlight"
        term="foo"
      >
        <div
          data-stylename="comment-meta"
        >
          <span
            data-stylename="person-name"
          >
            Joe Smith
          </span>
           commented on 
          <span
            data-stylename="post-title"
          >
            Awesome Sauce #hashtag
          </span>
        </div>
      </Highlight>
      <span
        data-stylename="date"
      >
        just now
      </span>
    </div>
    <CardImageAttachments
      attachments={
        Array [
          Object {
            "attachmentType": "image",
            "url": "jam.png",
          },
        ]
      }
      data-stylename="comment-images"
      linked={true}
    />
    <CardFileAttachments
      attachments={
        Array [
          Object {
            "attachmentType": "image",
            "url": "jam.png",
          },
        ]
      }
      data-stylename="comment-files"
    />
    <Connect(ClickCatcher)>
      <Highlight
        component="span"
        highlightClassName="highlight"
        term="foo"
      >
        <div
          dangerouslySetInnerHTML={
            Object {
              "__html": "<p>text of the comment. a long one. text of the comment. a long one. text of the comment. a long one. text of the comment. a long one. text of the …</p>",
            }
          }
          data-stylename="comment-body"
        />
      </Highlight>
    </Connect(ClickCatcher)>
    <div
      data-stylename="comment-footer"
    />
  </div>
</a>
`;

